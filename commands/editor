#!/bin/bash
#
# Auto generated by $ETC_APP, DO NOT EDIT!
#
# A shim script for running $EDITOR with normal user privileges.

EDITOR=${EDITOR:-$VISUAL}
[ -z "$EDITOR" ] && echo 'Missing $EDITOR or $VISUAL' && exit 1

if [ $(id -u) -ne 0 ]; then
    $EDITOR "$@"
else
    su $SUDO_USER -c "$EDITOR $@" # back to normal user.
fi
