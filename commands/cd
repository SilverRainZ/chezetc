#!/bin/bash
#
# Auto generated by $ETC_APP, DO NOT EDIT!
#
# A shim script for running cd with normal user privileges.

cd $CHEZMOI_SOURCE_DIR

if [ $(id -u) -ne 0 ]; then
    SHELL="${SHELL:-/bin/bash}"
    exec $SHELL
else
    # Get login shell, see also https://stackoverflow.com/a/11059152/4799273
    SHELL=$(getent passwd $SUDO_USER | cut -d: -f7)
    # Back to normal user.
    exec su $SUDO_USER -c "$SHELL $@"
fi
